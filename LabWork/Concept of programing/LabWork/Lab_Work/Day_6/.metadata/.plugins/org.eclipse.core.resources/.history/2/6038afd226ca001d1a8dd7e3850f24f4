package com.code;

public class Constructor {

	private int day;
	private int month;
	private int year;
	
	public Constructor () {
		
		System.out.println("-------Inside the default const---------");
		
	}
	
	
	
	
	
	public Constructor(int day, int month, int year) {
		
		
		this.day = day;
		this.month = month;
		this.year = year;
		
	
	}
	
	public void printdate() {
		
		System.out.println(day+"/"+month+"/"+year);
	}
	
	
	
}
