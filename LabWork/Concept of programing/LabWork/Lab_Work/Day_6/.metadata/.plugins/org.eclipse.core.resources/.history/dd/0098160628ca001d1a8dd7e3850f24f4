package Tester.code;

import java.util.Scanner;

import com.code.Constructor;

public class Testerconst {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		System.out.println("Entre the day / month/year ");
		Constructor obj1 = new Constructor(sc.nextInt(), sc.nextInt(), sc.nextInt());

		System.out.println(obj1);
		Constructor obj2 = new Constructor();
		System.out.println(obj2);
		
		Constructor obj3 = new Constructor(sc.nextInt(), sc.nextInt(), sc.nextInt());

		System.out.println(obj3);
		
		
		
		
		
	}

}
